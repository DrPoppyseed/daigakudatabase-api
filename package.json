{
  "name": "daigakudatabase-api",
  "author": "Haruki Jay Shimada <peaske16180@gmail.com> (https://github.com/DrPoppyseed)",
  "version": "0.2.1",
  "description": "⛏ Active development branch of DaigakuDatabase.com's API",
  "main": "dist/server/server.js",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "repository": {
    "url": "https://github.com/DrPoppyseed/daigakudatabase-api",
    "type": "git"
  },
  "engines": {
    "node": ">= 16.0"
  },
  "scripts": {
    "test": "jest",
    "build": "yarn lint:fix && tsc -p ./tsconfig.json",
    "start": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/server/server.js\"",
    "launch": "node ./dist/server/server.js",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --fix --ext .ts,.js .",
    "format": "prettier --write './**/*.{js,ts,md,json}' --config ./.prettierrc",
    "format:check": "prettier --check './**/*.{js,ts,md,json}' --config ./.prettierrc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:fix",
      "yarn format",
      "yarn test"
    ]
  },
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "express-validator": "^6.14.0",
    "firebase-admin": "^10.0.2",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.6",
    "morgan": "^1.10.0",
    "query-string": "^7.1.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.2"
  }
}
